<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.geometry.Insets?>

<GridPane  xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0"
           fx:controller="com.deligo.Frontend.Controllers.MainPage.MainTopPanelController">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
<!--aa-->
    <children>
        <Button text="EN" GridPane.columnIndex="1" GridPane.halignment="RIGHT" fx:id="btnEn" mnemonicParsing="false">
            <GridPane.margin>
                <Insets right="15.0" />
            </GridPane.margin>
        </Button>

        <Button text="%info" fx:id="openInfo" mnemonicParsing="false">
            <GridPane.margin>
                <Insets left="15.0" />
            </GridPane.margin>
        </Button>

        <Button text="SK" GridPane.columnIndex="1" GridPane.halignment="RIGHT" fx:id="btnSk" mnemonicParsing="false" >
            <GridPane.margin>
                <Insets right="55.0" />
            </GridPane.margin>
        </Button>

        <Button text="%login" GridPane.columnIndex="1" GridPane.halignment="RIGHT" fx:id="openLogin" mnemonicParsing="false">
            <GridPane.margin>
                <Insets right="95.0" />
            </GridPane.margin>
        </Button>
    </children>
</GridPane>