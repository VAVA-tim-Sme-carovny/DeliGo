<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.deligo.Frontend.Controllers.Employee.Admin.EditTableController"
            stylesheets="@/css/MainPage/EditTable.css"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <VBox spacing="15.0" alignment="TOP_CENTER"
              AnchorPane.topAnchor="20.0" AnchorPane.leftAnchor="20.0"
              AnchorPane.rightAnchor="20.0" AnchorPane.bottomAnchor="20.0">
            <padding>
                <Insets top="20" left="20" right="20" bottom="20"/>
            </padding>

            <Label text="Zadaj hodnotu:"/>
            <TextField fx:id="inputField" promptText="Sem napíš niečo..." prefWidth="300"/>

            <Button text="Potvrdiť" fx:id="submitButton"/>
            <Button text="Zmazať" fx:id="deleteButton"/>
        </VBox>
    </children>

</AnchorPane>
